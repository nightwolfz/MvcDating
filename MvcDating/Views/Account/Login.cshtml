@model MvcDating.Models.LoginModel

@{
    ViewBag.Title = "Log in";
}

<div class="col-md-12">
    <div class="col-md-7">
        @Html.Partial("_LoginForm", Model, new ViewDataDictionary { { "compactLoginForm", false } })
    </div>
    <div class="col-md-5">
        @Html.Action("ExternalLoginsList", new { ReturnUrl = ViewBag.ReturnUrl, showLoginForm = false })
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
